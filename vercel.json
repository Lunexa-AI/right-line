{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    },
    {
      "source": "/webhook",
      "destination": "/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ]
}
